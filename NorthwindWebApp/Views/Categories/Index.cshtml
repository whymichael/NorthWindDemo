@model IEnumerable<NorthwindWebApp.Models.Entities.Categories>
@{
    ViewBag.Title = "Category Index";
}
<h2>Index</h2>
<p>
    @Html.ActionLink("新增種類", "Create")
</p>
<table class="table">
    <tr>
        <th>
            種類名稱
        </th>
        <th>
            描述
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CategoryName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
        </tr>
    }
</table>
